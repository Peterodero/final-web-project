{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.jpeg",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.jpg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|txt))$",
      "dest": "/$1"
    },
    {
      "src": "^(?:\\/(?!\\.well-known(?:\\/|$)))(.*)$",
      "dest": "/index.html"
    }
  ]
}